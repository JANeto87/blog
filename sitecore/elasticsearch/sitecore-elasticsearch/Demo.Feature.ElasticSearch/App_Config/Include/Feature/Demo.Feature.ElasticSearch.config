<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>

    <settings>
      <setting name="ElasticSearch.Protocol" value="http"/>
      <setting name="ElasticSearch.Host" value="elastic.local"/>
      <setting name="ElasticSearch.Port" value="9200"/>
      <setting name="ElasticSearch.ArticlesIndex" value="articles-index"/>
    </settings>

    <commands>
      <command name="demo:elastic:indexing:refreshtree"
               type="Demo.Feature.ElasticSearch.Infrastructure.Commands.RefreshTree, Demo.Feature.ElasticSearch" />
      <command name="demo:elastic:indexing:recreateindexes"
               type="Demo.Feature.ElasticSearch.Infrastructure.Commands.ReCreateIndexes, Demo.Feature.ElasticSearch" />
    </commands>

  </sitecore>
</configuration>